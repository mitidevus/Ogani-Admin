<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta
            name="viewport"
            content="width=device-width, initial-scale=1, shrink-to-fit=no"
        />
        <meta name="description" content="" />
        <meta name="author" content="" />
        <title>Admin</title>
        <link
            rel="shortcut icon"
            type="image/x-icon"
            href="https://visualpharm.com/assets/381/Admin-595b40b65ba036ed117d3b23.svg"
        />
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
            crossorigin="anonymous"
        />
        <link rel="stylesheet" href="/css/tooplate-style.css" />
        <link rel="stylesheet" href="/css/notification.css" />
        <link rel="stylesheet" href="/css/styles.css" />
        <link rel="stylesheet" href="/css/my_style.css" />
    </head>
    <body>
        <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
            <!-- Sidebar Toggle-->
            <button
                class="btn btn-link btn-sm order-1 order-lg-0 ms-2 me-lg-0"
                id="sidebarToggle"
                href="#!"
            ><i class="fas fa-bars"></i></button>
            <!-- Navbar Brand-->
            <a class="navbar-brand ps-3" href="/">Admin</a>
            <!-- Navbar Search-->
            <form
                class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0"
            >
                <div class="input-group">
                    <input
                        hidden
                        class="form-control"
                        type="text"
                        placeholder="Tìm kiếm..."
                        aria-label="Search for..."
                        aria-describedby="btnNavbarSearch"
                    />
                    <button
                        hidden
                        class="btn btn-primary"
                        id="btnNavbarSearch"
                        type="button"
                    ><i class="fas fa-search"></i></button>
                </div>
            </form>

            <p class="navbar-text" style="margin-top: auto;">
                Welcome
                <b> Admin123</b>
            </p>

            <div class="notifications">
                <button type="button" class="icon_wrap">
                    <i class="fas fa-bell"></i>
                    <span style="font-size: 10px;" class="icon-button__badge">
                        1
                    </span>
                </button>
                <div class="notification_dd">
                    <h5 class="text-center mt-2">Thông báo</h5>
                    <hr class="mt-0 mb-0" />
                    <ul class="notification_ul">
                        <li class="starbucks success">
                            <div
                                class="d-flex justify-content-between align-items-center"
                            >
                                <div class="notify_data">
                                    <div class="title">Đặt hàng</div>
                                    <div class="sub_title">
                                        Khách hàng Đỗ Minh Trí đã đặt hàng lúc
                                        Jan 02 2022 08:32:51
                                    </div>
                                </div>
                                <div class="notify_status">
                                    <a title="chưa xem" href="#"><i
                                            class="fas fa-circle"
                                            style="font-size: 12px; color: rgb(5, 126, 173)"
                                        ></i></a>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="popup">
                <div class="shadow"></div>
                <div class="inner_popup">
                    <div class="notification_dd">
                        <ul class="all-notification_ul">
                            <li class="title">
                                <p>All Notifications</p>
                                <p class="close">
                                    <i
                                        class="fas fa-times"
                                        aria-hidden="true"
                                    ></i>
                                </p>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>

            <!-- Navbar-->
            <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
                <li class="nav-item dropdown">
                    <a
                        class="nav-link dropdown-toggle"
                        id="navbarDropdown"
                        href="#"
                        role="button"
                        data-bs-toggle="dropdown"
                        aria-expanded="false"
                    ><i class="fas fa-user fa-fw"></i></a>
                    <ul
                        class="dropdown-menu dropdown-menu-end"
                        aria-labelledby="navbarDropdown"
                    >
                        <li
                            class="d-flex align-items-center justify-content-around dropdown-item"
                        >
                            <i class="fas fa-user-circle"></i>
                            <a class="dropdown-item" href="/">Thông tin tài
                                khoản</a>
                        </li>
                        <li><hr class="dropdown-divider" /></li>
                        <li
                            class="d-flex align-items-center justify-content-around dropdown-item"
                        >
                            <i class="fas fa-user-edit"></i>
                            <a class="dropdown-item" href="/">Cập nhật tài khoản</a>
                        </li>
                        <li><hr class="dropdown-divider" /></li>
                        <li
                            class="d-flex align-items-center justify-content-around dropdown-item"
                        >
                            <i class="fas fa-users-cog"></i>
                            <a class="dropdown-item" href="/">Danh sách admin</a>
                        </li>
                        <li><hr class="dropdown-divider" /></li>
                        <li
                            class="d-flex align-items-center justify-content-around dropdown-item"
                        >
                            <i class="fas fa-user-plus"></i>
                            <a class="dropdown-item" href="/">Thêm admin</a>
                        </li>
                        <li><hr class="dropdown-divider" /></li>
                        <li
                            class="d-flex align-items-center justify-content-around dropdown-item"
                        >
                            <i class="fas fa-sign-out-alt"></i>
                            <a class="dropdown-item" href="/">Đăng xuất</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>

        <div id="layoutSidenav">
            <div id="layoutSidenav_nav">
                <nav
                    class="sb-sidenav accordion sb-sidenav-dark"
                    id="sidenavAccordion"
                >
                    <div class="sb-sidenav-menu">
                        <div class="nav">
                            <div class="sb-sidenav-menu-heading">Tài khoản</div>

                            <a class="nav-link" href="/accounts">
                                <div class="sb-nav-link-icon"><i
                                        class="fas fa-list-ul"
                                    ></i></div>
                                Xem danh sách
                            </a>

                            <div class="sb-sidenav-menu-heading">Sản phẩm</div>
                            <a class="nav-link" href="/product">
                                <div class="sb-nav-link-icon"><i
                                        class="fas fa-list-ul"
                                    ></i></div>
                                Xem danh sách
                            </a>

                            <div class="sb-sidenav-menu-heading">Danh mục</div>
                            <a class="nav-link" href="/category">
                                <div class="sb-nav-link-icon"><i
                                        class="fas fa-list-ul"
                                    ></i></div>
                                Xem danh sách
                            </a>

                            <div class="sb-sidenav-menu-heading">Thương hiệu</div>
                            <a class="nav-link" href="/producer">
                                <div class="sb-nav-link-icon"><i
                                        class="fas fa-list-ul"
                                    ></i></div>
                                Xem danh sách
                            </a>

                            <div class="sb-sidenav-menu-heading">Đơn đặt hàng</div>
                            <a class="nav-link" href="/order">
                                <div class="sb-nav-link-icon"><i
                                        class="fas fa-list-ul"
                                    ></i></div>
                                Xem danh sách
                            </a>

                        </div>
                    </div>
                </nav>
            </div>
            <div id="layoutSidenav_content">
                {{{body}}}
            </div>
        </div>

        <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
        <script
            src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js"
            crossorigin="anonymous"
        ></script>

        <script src="js/notification.js"></script>
        <script src="/js/search.js"></script>
        <script src="/js/scripts.js"></script>
    </body>
</html>